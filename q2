 #include<iostream>
 using namespace std;
 int main(){
    int arr[] = {64,34,25,12,22,11,90};
    int n = sizeof(arr)/sizeof(arr[0]);
    int i,j;
    for(i = 0;i<n;i++){
        for(j =0;j<n-1;j++){
            if(arr[j]>arr[j+1]){
                swap(arr[j],arr[j+1]);
            }
        }
    }

for(i = 0;i<n;i++){
    cout<<arr[i]<<" ";
}

    
    return 0;
 }
